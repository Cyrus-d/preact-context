    "@types/mocha": "^2.2.48",
    "@types/sinon": "^4.3.0",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "jsdom": "^11.9.0",
    "mocha": "^5.0.4",
    "npm-run-all": "^4.1.2",
    "preact": "^8.2.7",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.6",
    "sinon-chai": "^3.0.0",
    "typescript": "^2.7.2"
  }
}
